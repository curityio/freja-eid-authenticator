<script type="text/javascript" $!nonceAttr>
    /**
     * Gets the URI that the differences browsers need to start the BankID app and resume correctly after
     * authenticating with it
     *
     * @returns string auto-start URI for BankID
     */
    function getAutoStartUri() {
        // You can append &originAppScheme=my.custom.scheme:// in the uri returned by this method
        // so the Freja e-id app can automatically return to your application once authentication is done or cancelled
        return "$!_thisDeviceLink"
    }
</script>
